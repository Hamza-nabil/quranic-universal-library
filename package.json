{
  "name": "tarteel-qul",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "^3.0.1",
    "@hotwired/turbo-rails": "^7.1.0",
    "@json-editor/json-editor": "^2.10.1",
    "@meforma/vue-toaster": "^1.3.0",
    "@popperjs/core": "^2.11.0",
    "@rails/actiontext": "^6.1.4-1",
    "@syncfusion/ej2-layouts": "^22.1.37",
    "@vueform/slider": "^2.0.8",
    "bootstrap": "5.1.3",
    "copy-to-clipboard": "^3.3.3",
    "esbuild": "^0.14.2",
    "esbuild-plugin-vue3": "^0.3.0",
    "hotkeys-js": "^3.8.7",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.13.2",
    "jquery-ujs": "^1.2.3",
    "local-time": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "raphael": "^2.3.0",
    "sass": "^1.44.0",
    "select2": "^4.1.0-rc.0",
    "simplify-js": "^1.2.4",
    "snapsvg": "^0.5.1",
    "svg-points": "^6.0.1",
    "toastr": "^2.1.4",
    "trix": "^1.3.1",
    "utf8": "^3.0.0",
    "vue": "^3.2.24",
    "vuex": "^4.0.2"
  },
  "scripts": {
    "build:css": "npm-run-all --parallel \"build:css:* {@}\" --",
    "build:css:app": "sass ./app/assets/stylesheets/application.scss ./app/assets/builds/application.css --no-source-map --load-path=node_modules --style=compressed",
    "build:css:admin": "sass ./app/assets/stylesheets/active_admin.scss ./app/assets/builds/active_admin.css --no-source-map --load-path=node_modules --style=compressed",
    "build:css:pdf": "sass ./app/assets/stylesheets/pdf.scss ./app/assets/builds/pdf.css --no-source-map --load-path=node_modules --style=compressed",
    "build:css:tinymce_custom": "sass ./app/assets/stylesheets/tinymce_custom.scss ./app/assets/builds/tinymce_custom.css --no-source-map --load-path=node_modules --style=compressed",
    "build": "esbuild app/javascript/*.js --bundle --sourcemap --outdir=app/assets/builds --loader:.svg=text",
    "build:segments": "node build_segments_bundle.js",
    "build:esm": "esbuild app/javascript/*.* --bundle --loader=vue --outdir=app/assets/builds --splitting --format=esm"
  }
}
